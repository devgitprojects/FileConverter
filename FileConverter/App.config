<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.diagnostics>
      <sources>
        <source name="FileConverter" switchType="System.Diagnostics.SourceSwitch" switchValue="All"> 
          <listeners>
            <add name="FileConverterListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="Log.log" traceOutputOptions="ThreadId, DateTime"/>            
            <remove name="Default"/>
          </listeners>
        </source>
      </sources>      
    </system.diagnostics>
</configuration>